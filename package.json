{
  "name": "@circles/core",
  "version": "2.10.8",
  "description": "Common methods to interact with the Circles ecosystem",
  "main": "lib/index.js",
  "contributors": [
    "adzialocha"
  ],
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/CirclesUBI/circles-core.git"
  },
  "scripts": {
    "build": "npm run clear && rollup -c",
