    "rollup-plugin-uglify": "^6.0.4",
    "serialize-javascript": "^5.0.1",
    "truffle": "^5.5.13",
    "web3": "^1.7.3"
  },
  "peerDependencies": {
    "isomorphic-fetch": "^3.0.0",
    "web3": "^1.7.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@circles/safe-contracts": "^1.0.17",
