    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "serialize-javascript": "^5.0.1",
    "truffle": "^5.1.48",
    "web3": "^1.3.0"
  },
  "peerDependencies": {
    "isomorphic-fetch": "^3.0.0",
    "web3": "^1.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@circles/safe-contracts": "^1.0.17",
    "circles-contracts": "^2.3.1"
