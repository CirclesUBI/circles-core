{
  "name": "@circles/core",
  "version": "2.11.1",
  "description": "Common methods to interact with the Circles ecosystem",
  "main": "lib/index.js",
  "contributors": [
    "adzialocha"
  ],
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
